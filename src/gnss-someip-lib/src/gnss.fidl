package gnss

import gnss.common.* from "types.fidl"

interface TimeServer {
    version { major 0 minor 1 }

    broadcast now {
        out {
            Time time
        }
    }

    broadcast position {
        out {
            Position pos
        }
    }

    method setCorrection {
        in {
            UInt32 value
        }
    }
} 